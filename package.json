{
  "name": "slippi-launcher",
  "productName": "Slippi Launcher",
  "description": "Launch Slippi Online, browse and watch saved replays",
  "license": "GPL-3.0",
  "scripts": {
    "build": "concurrently \"npm run build:main\" \"npm run build:renderer\" \"npm run build:migrations\" \"npm run i18n:convert\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
    "build:migrations": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.migrations.prod.ts",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "lint": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "clean": "ts-node ./.erb/scripts/clean.js dist",
    "package": "npm run clean && npm run build && electron-builder build --publish never",
    "postinstall": "patch-package && ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
    "dev": "ts-node ./.erb/scripts/check-port-in-use.js && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts --env mock",
    "start": "ts-node ./.erb/scripts/check-port-in-use.js && npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electronmon ./.erb/scripts/start-main.js",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "test": "npm run build:migrations && cross-env ELECTRON_RUN_AS_NODE=true electron ./node_modules/vitest/vitest.mjs --run",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "i18n:extract": "i18next-auto-keys extract --include \"src/renderer/**/*.messages.ts\"",
    "i18n:sync": "i18next-auto-keys sync",
    "i18n:convert": "i18next-auto-keys convert --batch --output ./release/app/dist/renderer/i18n",
    "i18n:status": "i18next-auto-keys status --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project-slippi/slippi-launcher.git"
  },
  "author": {
    "name": "Jas Laferriere",
    "email": "jas.laferriere@gmail.com",
    "url": "https://github.com/jlaferri"
  },
  "engines": {
    "node": ">=20"
  },
  "browserslist": [
    "chrome 91"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --quiet"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --cache --fix"
  },
  "i18next-auto-keys": {
    "poTemplateName": "messages.pot",
    "poOutputDirectory": "./locales",
    "argMode": "indexed",
    "projectId": "slippi-launcher 1.0"
  },
  "electronmon": {
    "patterns": [
      "!src/renderer/**/*",
      "!**/*.md",
      "!**/*.spec.ts",
      "!**/*.test.ts"
    ]
  },
